                  <tr>
                      <th>Name</th>
                      <th>Category</th>
                      <th>Price</th>
                      <th>Theater Name</th>
                      <th>Date Time</th>
                      <th>Confirm</th>
                      <th>Delete</th>
                  </tr>
              </thead>
              <tbody>
                <tr ng-repeat="cartedEvent in cartedEvents track by $index">
                  <td>{{cartedEvent.title}}</td>
                  <td>{{cartedEvent.type}}</td>
